{
    "name": "bendev/laravel-lexoffice-api",
    "description": "Laravel package for lexoffice.de Public API",
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^6.3 || ^7.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Benedict Humpert",
            "email": "benedicthumpert@gmail.com"
        }
    ],
    "keywords": [
